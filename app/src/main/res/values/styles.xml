<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="authBtnStyle" parent="ShapeAppearanceOverlay.Material3.Button">
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>
    <style name="authGoogleBtnStyle" parent="ShapeAppearanceOverlay.Material3.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/btnGoogleAuth</item>
    </style>
    <style name="ToolbarTitleFont" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="fontFamily">@font/one_slice</item>
        <item name="android:textSize">35sp</item>
    </style>
    <style name="MenuImageButton" parent="Widget.AppCompat.ImageButton">
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">250dp</item>
        <item name="android:background">@color/fui_transparent</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:padding">8dp</item>
    </style>
</resources>